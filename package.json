{
  "name": "@zapshark/zapi",
  "version": "0.1.0",
  "description": "Zapshark ZAPI Framework - A lightweight and flexible Node.js framework for building RESTful APIs with Express and Mongoose.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zapshark/zapi.git"
  },
  "type": "commonjs",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "scripts": {
    "dev": "node src/example/index.js",
    "env:scan": "node src/bin/zapi-env-scan.js",
    "generate:apiKey": "node src/bin/zapi-keygen.js gen"
  },
  "keywords": [
    "zapi",
    "framework",
    "nodejs",
    "zapshark",
    "api",
    "express",
    "mongoose",
    "rest",
    "http"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Zapshark Technologies",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.2",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "mongoose": "^8.6.1",
    "ws": "^8.18.3"
  }
}
